// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Copic {
  id          Int @id @default(autoincrement())
  colorName   String
  colorCode   String
  type        String
  number      Int
  fileName    String
  createdAt   DateTime @default(now())
}
